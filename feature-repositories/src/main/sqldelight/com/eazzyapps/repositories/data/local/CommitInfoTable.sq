CREATE TABLE CommitInfoLocal (
sha TEXT NOT NULL PRIMARY KEY ON CONFLICT REPLACE,
repoId INTEGER NOT NULL,
date TEXT NOT NULL,
email TEXT NOT NULL,
name TEXT NOT NULL,
message TEXT NOT NULL
);

selectByRepoId:
SELECT *
FROM CommitInfoLocal
WHERE repoId=?;

insert:
INSERT INTO CommitInfoLocal
VALUES ?;

deleteAll:
DELETE
FROM CommitInfoLocal;